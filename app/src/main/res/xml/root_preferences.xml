<androidx.preference.PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">



    <PreferenceCategory app:title="Payment Settings">

        <CheckBoxPreference
            app:key="cod_enabled"
            app:title="Cash On Delivery Enabled" />


        <CheckBoxPreference
            app:key="pod_enabled"
            app:title="Pay On Delivery Enabled" />


        <CheckBoxPreference
            app:key="razorpay_enabled"
            app:title="RazorPay Enabled" />


    </PreferenceCategory>




    <PreferenceCategory app:title="Commission and Tax Rate">

        <CheckBoxPreference
            app:key="market_commission"
            app:title="Market Commission" />


        <CheckBoxPreference
            app:key="pod_enabled"
            app:title="Pay On Delivery Enabled" />


        <CheckBoxPreference
            app:key="razorpay_enabled"
            app:title="RazorPay Enabled" />


    </PreferenceCategory>




</androidx.preference.PreferenceScreen>
