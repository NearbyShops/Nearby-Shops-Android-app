<?xml version="1.0" encoding="utf-8"?>
<resources>


<!--    http://markets-blr.nearbyshops.org:5021-->
<!--    http://192.168.43.233:5121-->


    <!-- Your instance url goes here. Connect your app to your instance. In case multi-market mode is enabled this URL will be ignored. -->
    <string name="custom_url">http://markets-blr.nearbyshops.org:5021</string>
    <bool name="multi_market_enabled">false</bool>



    <!-- If multi-market mode is disabled then simply ignore this URL. It tells you url to SDS server which enables multi-market mode. -->
    <string name="sds_url">@string/service_url_sds_local_hotspot</string>



    <!-- App name that you see on your front screen ( shops list screen ) -->
    <string name="app_name">Farmers Market</string>
    <drawable name="custom_icon">@mipmap/blue_icon</drawable>




    <!-- Set your custom app colors here -->
    <color name="color_primary_custom">@color/mapboxBlue</color>
    <color name="color_primary_dark_custom">@color/buttonColorDark</color>
    <color name="color_accent_custom">@color/gplus_color_2</color>




    <!-- Enable maps and Places API and Mapbox map here -->
    <string name="google_maps_api_key">abcde</string>
    <string name="custom_mapbox_url">https://example.com</string>



    <!--Switch between Google Maps or Mapbox API by using this setting-->
    <bool name="use_google_maps">true</bool>



    <!--It is mandatory to provide terms of service and privacy policy link to publish your app on google play-->
    <string name="tos_link">https://example.com/tos.html</string>
    <string name="privacy_policy_link">https://example.com/pivacy_policy.html</string>
    <string name="faqs_link">https://example.com/faq_link.html</string>



    <!-- Other Settings -->

    <!-- To enable single vendor mode set single vendor mode to true and set shop id to the shop which you want to set as your default
    shop. Please note that the Shop must be enabled to be visible by the customers. -->
    <bool name="single_vendor_mode_enabled">false</bool>
    <integer name="single_vendor_shop_id">33</integer>


    <!-- set whether you want to show create shop option in the front screen (shops list) screen of the market-->
    <bool name="show_create_shop">true</bool>

    <!-- You may want to disable items screen in the multi-vendor app. You can do it with this setting !-->
    <bool name="show_items_screen_in_multi_vendor">true</bool>



    <!-- When enabled login through SMS-OTP will be available only on national phone numbers of the default country set by the market admin  !-->
    <bool name="restrict_phone_login_country">true</bool>




    <!--Enable or Disable Phone or Email for login based on your Preferences -->
    <!--Please ensure at least one mode of registration is Enabled -->
    <bool name="login_using_email_enabled">true</bool>
    <bool name="login_using_phone_enabled">true</bool>



    <!-- Few constants ... you can ignore them -->
    <string name="service_url_sds">http://sds.nearbyshops.org</string>
    <string name="service_url_sds_demo">http://sds.nearbyshops.org:5000</string>
    <string name="service_url_sds_local_hotspot">http://192.168.43.233:5125</string>

</resources>
